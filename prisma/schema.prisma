generator client {
  provider = "prisma-client"
}

datasource db {
  provider = "postgresql"
}

model City {
  id        Int     @id @default(autoincrement())
  nameUa    String
  nameRu    String
  nameEn    String
  region    String
  countryUa String
  countryEn String
  latitude  Float
  longitude Float

  @@unique([nameEn])
  @@index([nameUa])
  @@index([nameRu])
  @@index([region])
}
