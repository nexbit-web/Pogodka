generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model City {
  id        Int    @id @default(autoincrement())
  nameUa    String
  nameRu    String
  nameEn    String @unique
  region    String
  countryUa String
  countryEn String
  latitude  Float
  longitude Float
  slug      String @unique

  @@index([nameUa])
  @@index([nameRu])
  @@index([region])
}
